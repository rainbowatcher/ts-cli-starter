{
  "name": "ts-cli-starter",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.6.3",
  "description": "",
  "author": "rainbowatcher <rainbow-w@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/rainbowatcher/ts-cli-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rainbowatcher/ts-cli-starter.git"
  },
  "bugs": {
    "url": "https://github.com/rainbowatcher/ts-cli-starter/issues"
  },
  "keywords": [
    "ts-cli-starter"
  ],
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "sum": "./bin/cmd.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pkgroll",
    "lint": "eslint .",
    "prepare": "husky install",
    "release": "bumpp && npm publish",
    "start": "jiti src/index.ts",
    "test": "uvu -r jiti/register test",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "^0.6.3",
    "cac": "^6.7.14"
  },
  "devDependencies": {
    "@rainbowatcher/eslint-config": "0.2.4",
    "@types/node": "^18.16.18",
    "bumpp": "^9.1.1",
    "commitlint": "^17.6.6",
    "eslint": "^8.43.0",
    "husky": "^8.0.3",
    "jiti": "^1.18.2",
    "lint-staged": "^13.2.2",
    "pkgroll": "^1.10.0",
    "typescript": "^5.1.3",
    "uvu": "^0.5.6"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue,json}": "eslint --fix"
  }
}
