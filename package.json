{
  "name": "ts-cli-starter",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.10.2",
  "description": "",
  "author": "rainbowatcher <rainbow-w@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/rainbowatcher/ts-cli-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rainbowatcher/ts-cli-starter.git"
  },
  "bugs": {
    "url": "https://github.com/rainbowatcher/ts-cli-starter/issues"
  },
  "keywords": [
    "ts-cli-starter"
  ],
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "sum": "./bin/cmd.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pkgroll",
    "lint": "eslint .",
    "prepare": "husky install",
    "release": "bumpp && npm publish",
    "start": "jiti src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "cac": "^6.7.14"
  },
  "devDependencies": {
    "@rainbowatcher/eslint-config": "0.3.3",
    "@types/node": "^18.18.8",
    "bumpp": "^9.2.0",
    "commitlint": "^18.2.0",
    "eslint": "^8.52.0",
    "husky": "^8.0.3",
    "jiti": "^1.21.0",
    "lint-staged": "^15.0.2",
    "pkgroll": "^2.0.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue,json}": "eslint --fix"
  }
}
