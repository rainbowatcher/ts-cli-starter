{
    "name": "ts-cli-starter",
    "type": "module",
    "version": "0.0.0",
    "packageManager": "pnpm@9.10.0",
    "description": "",
    "author": "rainbowatcher <rainbow-w@qq.com>",
    "license": "MIT",
    "homepage": "https://github.com/rainbowatcher/ts-cli-starter#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rainbowatcher/ts-cli-starter.git"
    },
    "bugs": {
        "url": "https://github.com/rainbowatcher/ts-cli-starter/issues"
    },
    "keywords": [
        "ts-cli-starter"
    ],
    "sideEffects": false,
    "main": "./dist/index.cjs",
    "typesVersions": {
        "*": {
            "*": [
                "./dist/*",
                "./dist/index.d.ts"
            ]
        }
    },
    "bin": {
        "sum": "./bin/cmd.js"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "lint": "eslint .",
        "prepare": "simple-git-hooks",
        "changelog": "conventional-changelog -si CHANGELOG.md",
        "release": "bumpp --all -x 'pnpm changlog' && npm publish",
        "start": "node bin/cli.js",
        "dev": "tsx src/index.ts",
        "test": "vitest",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@clack/prompts": "^0.7.0",
        "cac": "^6.7.14"
    },
    "devDependencies": {
        "@commitlint/config-conventional": "^19.5.0",
        "@rainbowatcher/eslint-config": "0.16.0",
        "@rainbowatcher/eslint-config-ignore": "0.16.0",
        "@rainbowatcher/eslint-config-js": "0.16.0",
        "@rainbowatcher/eslint-config-json": "0.16.0",
        "@rainbowatcher/eslint-config-md": "0.16.0",
        "@rainbowatcher/eslint-config-prettier": "0.16.0",
        "@rainbowatcher/eslint-config-ts": "0.16.0",
        "@types/node": "^22.5.5",
        "bumpp": "^9.5.2",
        "commitlint": "^19.5.0",
        "conventional-changelog-cli": "^5.0.0",
        "eslint": "^9.10.0",
        "lint-staged": "^15.2.10",
        "simple-git-hooks": "^2.11.1",
        "tsup": "^8.3.0",
        "tsx": "^4.19.1",
        "typescript": "^5.6.2",
        "vitest": "^2.1.1"
    },
    "simple-git-hooks": {
        "pre-commit": "npx lint-staged",
        "commit-msg": "npx commitlint --edit $1"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,vue,json}": "eslint"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    }
}
